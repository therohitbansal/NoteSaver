  "license": "ISC",
  "proxy": "https://localhost:4000",
  "dependencies": {
    "16": "^0.0.2",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.18",
    "nodemon": "^2.0.22"
  }
}
