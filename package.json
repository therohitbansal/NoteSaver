{
  "name": "keeper",
  "version": "1.0.0",
  "description": "Keeper - A note taking app",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "build": "npm run build",
    "start": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n \"server,client\" -c \"red,green\" \"nodemon server.js\" \"npm run client\""
  },
